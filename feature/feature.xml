<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.github.generategwtrfproxy.feature"
      label="GenerateGwtRfProxyFeature"
      version="0.0.3">

   <description url="https://github.com/nmorel/GenerateGwtRfProxy">
      Eclipse plugin to generate the GWT RequestFactory proxy
   </description>

   <url>
      <update label="GenerateGwtRfProxy Update Site" url="https://raw.github.com/nmorel/GenerateGwtRfProxy/master/update"/>
   </url>

   <requires>
      <import plugin="org.eclipse.ui" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.ui" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.debug.ui" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface.text" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.6.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="com.github.generategwtrfproxy.plugin"
         download-size="0"
         install-size="0"
         version="0.0.3"
         unpack="false"/>

</feature>
